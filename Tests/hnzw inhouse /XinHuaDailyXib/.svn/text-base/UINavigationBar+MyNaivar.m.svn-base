#import "UINavigationBar+MyNaivar.h"

@implementation UINavigationBar (MyNaivar)


-(void)drawRect:(CGRect)rect{
    
    UIImageView* uiv = [[UIImageView alloc] initWithFrame:rect];
    [uiv setImage:[UIImage imageNamed:@"titlebg.png"]];
    [uiv drawRect:rect];
    [uiv release];
}

@end
